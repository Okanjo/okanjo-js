{
  "name": "okanjo-js",
  "homepage": "https://okanjo.com/",
  "version": "2.0.0",
  "description": "Okanjo JavaScript Widget Framework",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Okanjo/okanjo-js.git"
  },
  "scripts": {
    "build": "node_modules/.bin/gulp full_build",
    "hint": "node_modules/.bin/eslint .",
    "test": "node_modules/.bin/mocha test",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha --exit",
    "report": "npm run build && npm run cover && npm run hint"
  },
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "keywords": [
    "Okanjo",
    "SmartServe",
    "Farm",
    "PLU",
    "Ads"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "async": "^3.2.3",
    "clean-css": "^5.2.4",
    "del": "^6.0.0",
    "eslint": "^8.11.0",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^6.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-file-include": "kfitzgerald/gulp-file-include#0.8.0",
    "gulp-insert": "^0.5.0",
    "gulp-less": "^5.0.0",
    "gulp-notify": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-size": "^4.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-umd": "^2.0.0",
    "gulp-wrap": "^0.15.0",
    "html-minifier": "^4.0.0",
    "nyc": "^15.1.0",
    "js-string-escape": "^1.0.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "less-plugin-autoprefix": "^2.0.0",
    "mocha": "^9.2.2",
    "mustache": "^4.2.0",
    "qs": "^6.10.3",
    "raven-js": "~3.27.2",
    "should": "^13.2.3"
  },
  "mocha": {
    "require": ["should", "./test/env.js"],
    "reporter": "spec",
    "checkLeaks": false,
    "timeout": 2000
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "/node_modules/**",
      "/build/**",
      "/dist/**",
      "/lib/**",
      "/local-test/**",
      "/coverage/**",
      "/.nyc_output/**",
      "/old/**"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "script"
    },
    "plugins": [],
    "extends": "eslint:recommended",
    "globals": {
      "require": true,
      "module": true,
      "describe": true,
      "it": true,
      "before": true,
      "after": true,
      "afterEach": true,
      "Promise": true,
      "okanjo": true,
      "define": true
    },
    "overrides": [
      {
        "files": [
          "docs/**"
        ],
        "rules": {
          "no-console": "off",
          "no-unused-vars": "off"
        }
      },
      {
        "files": [
          "test/*.js"
        ],
        "parserOptions": {
          "sourceType": "script",
          "no-console": "off"
        }
      }
    ]
  }
}
